---
import TrainingProgramLayout from "../../layouts/TrainingProgramLayout.astro";
import instrument from "../../data/instrumentRatingData";
---

<TrainingProgramLayout
  bannerImagePath={instrument.bannerImagePath}
  imagePath={instrument.imagePath}
  title={instrument.title}
  subtitle={instrument.subtitle}
  locations={instrument.locations}
  description={instrument.description}
  prerequisites={instrument.prerequisites}
  enrollmentProcess={instrument.enrollmentProcess}
  tiers={instrument.tiers}
  futurePathways={instrument.futurePathways}
/>
